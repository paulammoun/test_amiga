								"descendingImage": "svgIcon=#alpha-icon-chevronDown:icon",
								"className": ""
							},
							"resize": {
								"location": "after",
								"className": "listColHeaderItemResize",
								"handle": {
									"className": "",
									"size": "15px"
								}
							}
						}
					},
					"data": {
						"item": {
							"className": "listItemData"
						}
					}
				},
				"view": {
					"navigation": {
						"prev": {
							"className": "listNavButtonPrev",
							"pressedClassName": "listNavButtonPressed"
